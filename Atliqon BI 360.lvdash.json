{
  "datasets": [
    {
      "name": "10bc733b",
      "displayName": "vw_fact_orders_enriched",
      "queryLines": [
        "SELECT * FROM fmcg.gold.vw_fact_orders_enriched"
      ]
    },
    {
      "name": "5745a1f7",
      "displayName": "Untitled dataset",
      "queryLines": [
        "WITH monthly AS (\r\n",
        "  SELECT \r\n",
        "    date_format(date, 'yyyy-MM') AS month_key,\r\n",
        "    SUM(total_amount_inr) AS revenue\r\n",
        "  FROM fmcg.gold.vw_fact_orders_enriched\r\n",
        "  GROUP BY date_format(date, 'yyyy-MM')\r\n",
        ")\r\n",
        "SELECT \r\n",
        "  m.month_key,\r\n",
        "  m.revenue,\r\n",
        "  ROUND(\r\n",
        "    100.0 * (m.revenue - prev.revenue) / NULLIF(prev.revenue, 0), \r\n",
        "    2\r\n",
        "  ) AS mom_pct_change\r\n",
        "FROM monthly m\r\n",
        "LEFT JOIN monthly prev\r\n",
        "  ON prev.month_key = date_format(\r\n",
        "    add_months(to_date(concat(m.month_key, '-01')), -1), \r\n",
        "    'yyyy-MM'\r\n",
        "  )\r\n",
        "ORDER BY m.month_key DESC\r\n",
        "LIMIT 12;"
      ]
    }
  ],
